let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +22 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_about.html
badd +133 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_base.css
badd +28 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_pages.txt
badd +41 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout1.css
badd +130 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css
badd +34 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/test.css
badd +30 ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_reviews.html
argglobal
%argdel
edit ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 103 + 103) / 207)
exe 'vert 2resize ' . ((&columns * 103 + 103) / 207)
argglobal
balt ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_base.css
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=1
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 232 - ((0 * winheight(0) + 24) / 48)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 232
normal! 0
lcd ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial
wincmd w
argglobal
if bufexists(fnamemodify("~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css", ":p")) | buffer ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css | else | edit ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css | endif
if &buftype ==# 'terminal'
  silent file ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/ct_layout2.css
endif
balt ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial/test.css
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=1
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 1 - ((0 * winheight(0) + 24) / 48)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 1
normal! 0
lcd ~/Dropbox/DC/classes/CSIS1280_html/data_files/html01/tutorial
wincmd w
2wincmd w
exe 'vert 1resize ' . ((&columns * 103 + 103) / 207)
exe 'vert 2resize ' . ((&columns * 103 + 103) / 207)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
let g:this_fzf_session =v:this_session
